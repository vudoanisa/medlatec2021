@using MEDLATEC.BusinessLayer;
@using System.Data;
@using CaptchaMvc.HtmlHelpers;
@using System.Text.RegularExpressions;
@{
    Layout = null;
    var NewsDichvu = (List<Cms_News>)ViewBag.NewsDichVu;
    var NewsCate = (List<Cms_News>)ViewBag.NewsCate;
    var TinNoiBat = (List<Cms_News>)ViewBag.TinNoiBat;
    var TinLienQuan = (List<Cms_News>)ViewBag.TinLienQuan;
    var NewsTags = (List<Cms_NewsTags>)ViewBag.Tag;
}
<!doctype html>
<html amp lang="vi">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, minimum-scale=1, initial-scale=1" />




    <title>@Html.Raw(@NewsDichvu[0].NewsName.ToString().TrimStart().TrimEnd().Replace("\"", "")) | Medlatec</title>
    @if (NewsDichvu[0].NewsDescription != null)
    {
        <meta name="description" content="@Html.Raw(@NewsDichvu[0].NewsDescription.ToString().TrimStart().TrimEnd().Replace("\"", ""))" />

    }
    @if (NewsDichvu[0].NewsName != null)
    {
        <meta name="title" content="@Html.Raw(@NewsDichvu[0].NewsName.ToString().TrimStart().TrimEnd().Replace("\"", ""))" />

    }
    @if (NewsDichvu[0].NewsKeyword != null)
    {
        <meta name="keywords" content="@Html.Raw(@NewsDichvu[0].NewsKeyword.ToString().TrimStart().TrimEnd())" />
    }
    else
    {
        <meta name="keywords" content="@Html.Raw(@NewsDichvu[0].NewsDescription.ToString().TrimStart().TrimEnd())" />
    }

    @if (@NewsDichvu[0].NewsImages != null)
    {
        <meta property="og:image" content="@NewsDichvu[0].NewsImages" />
    }
    else
    {
        <meta property="og:image" content="https://medlatec.vn/Content/images/logo.jpg" />
    }


    <meta name="robots" content="index,follow,all">
    <link rel="canonical" href="https://medlatec.vn/@CMS_Core.Common.Common.GetURLDetailByNews("tin-tuc",CMS_Core.Common.Common.removeCharEncode(NewsDichvu[0].NewsName.ToLower().TrimEnd().TrimStart().ToString()), "s" + NewsDichvu[0].CateId.ToString(), "n" + NewsDichvu[0].NewsId.ToString())" />

    <link rel="apple-touch-icon" sizes="180x180" href="/Content/images/logochuan-01.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/Content/images/logochuan-32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/Content/images/logochuan-16.png">

