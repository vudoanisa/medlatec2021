@using MEDLATEC.BusinessLayer;

@using System.Data;
@{


    Layout = "~/Views/Shared/_Layout.cshtml";
    var News = (List<Cms_News>)ViewBag.News;
    var NewsDichvu = (List<Cms_News>)ViewBag.NewsDichVu;
    var Video = (List<Cms_Video>)ViewBag.Video;
