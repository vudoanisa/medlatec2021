                            </a>
                        </div>
                        <div class="news-box--content col-6 d-block d-lg-none">
                            <h3>
                                <a href="/@CMS_Core.Common.Common.GetURLDetailByNews("tin-tuc",@NewsDichvu[0].NewsName.ToString().ToLower() ,"s"+@NewsDichvu[0].CateId.ToString() ,"n"+@NewsDichvu[0].NewsId.ToString())" title="@NewsDichvu[0].NewsName.ToString()">
                                    @NewsDichvu[0].NewsName.ToString()
                                </a>
                            </h3>
                            <div class="news-date"><i class="news-calendar"></i>Ngày @NewsDichvu[0].DateCreate.ToString("dd/MM/yyyy")</div>
                        </div>
                    </div>
                </div>
                <div class="news-box--col col-lg-8">
                    <div class="news-box--list">
                        @if (NewsDichvu.Count > 0)
                        {
                            <div class="news-box--item news-box--first d-none d-lg-block">
                                <h3 class="news-ud">
                                    <a href="/@CMS_Core.Common.Common.GetURLDetailByNews("tin-tuc",@NewsDichvu[0].NewsName.ToString().ToLower(),"s"+@NewsDichvu[0].CateId.ToString() ,"n"+@NewsDichvu[0].NewsId.ToString())" title="@NewsDichvu[0].NewsName.ToString()">
                                        @NewsDichvu[0].NewsName.ToString()
                                    </a>
                                </h3>
                                <div class="new-desc">@NewsDichvu[0].NewsDescription.ToString()</div>
                                <div class="news-date"><i class="news-calendar"></i>Ngày @NewsDichvu[0].DateCreate.ToString("dd/MM/yyyy")</div>
                            </div>



                            for (int i = 1; i < 5; i++)
                            {
                                <div class="news-box--item">
                                    <h3><a href="/@CMS_Core.Common.Common.GetURLDetailByNews("tin-tuc",@NewsDichvu[i].NewsName.ToString().ToLower(),"s"+@NewsDichvu[i].CateId.ToString(),"n"+@NewsDichvu[i].NewsId.ToString())" title="@NewsDichvu[i].NewsName.ToString()">@NewsDichvu[i].NewsName.ToString()</a></h3>
                                </div>
                            }
                        }



                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

