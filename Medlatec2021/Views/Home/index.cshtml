}
@section Note {
    <title>Bệnh viện đa khoa MEDLATEC | Lấy mẫu xét nghiệm tại nhà</title>

    <meta name="description" content="@Html.Raw("Hệ thống bệnh viện đa khoa MEDLATEC với đội ngũ y bác sĩ giỏi giàu kinh nghiệm, trang thiết bị y tế hàng đầu Việt Nam. Dịch vụ khám chữa bệnh đầy đủ chuyên khoa: Xét nghiệm lấy mẫu máu tại nhà, chẩn đoán hình ảnh, chế độ bảo hiểm y tế toàn diện, khám sức khoẻ doanh nghiệp")" />


    <meta name="title" content="@Html.Raw("Bệnh viện đa khoa MEDLATEC | Lấy mẫu xét nghiệm tại nhà")" />

    <meta name="keywords" content="@Html.Raw("Hệ thống bệnh viện đa khoa MEDLATEC với đội ngũ y bác sĩ giỏi giàu kinh nghiệm, trang thiết bị y tế hàng đầu Việt Nam. Dịch vụ khám chữa bệnh đầy đủ chuyên khoa: Xét nghiệm lấy mẫu máu tại nhà, chẩn đoán hình ảnh, chế độ bảo hiểm y tế toàn diện, khám sức khoẻ doanh nghiệp")" />

