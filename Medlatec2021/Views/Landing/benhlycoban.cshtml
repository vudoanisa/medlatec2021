                        @Html.TextBoxFor(m => m.txthoten, new { @class = "form-control-sm", placeholder = "Họ tên (*)", type = "text" })
                        @Html.TextBoxFor(m => m.txtdt, new { @class = "form-control-sm", placeholder = "Số điện thoại (*)", type = "number" })

