        @using (Html.BeginForm("BenhHoHap", "Landing", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal" }))
        {
            @Html.AntiForgeryToken()
            <footer id="formInput">
                <div class="container fix-container">
                    <div class="from-input">
                        <div class="input-form">
                            <form action="">
                                @Html.TextBoxFor(m => m.txthoten, new { @class = "form-control inputtext TenKhachHang", autocomplete = "off", placeholder = "Họ tên khách hàng(*)", type = "text" })
                                @Html.TextBoxFor(m => m.txtdt, new { @class = "form-control inputtext Sodt", autocomplete = "off", placeholder = "Số điện thoại(*)", type = "text" })
                                @Html.TextBoxFor(m => m.txtnote, new { @class = "form-control inputtext goikham", autocomplete = "off", placeholder = "Ghi chú", type = "text" })

                            </form>
                            @if (!string.IsNullOrEmpty(Model.txtError))
                            {
                                if (Model.txtError.Length > 0)
                                {

                                    <span class="text-danger" style="margin-bottom:10px; background-color:white;">
                                        @Model.txtError
                                    </span>

                                }
                            }

                            <div class="submit-from-data">
                                <button type="submit" name="submit" class="btn btn-submit--from" id="IDfromsubmit" value="SaveAdd">Đăng Ký</button>


                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        }


