<body>
    @using (Html.BeginForm("kyniem25", "Landing", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal" }))
    {
        @Html.AntiForgeryToken()
        <section>
            <div class="sec1">
                <div class="container">
                    <div class="row">
                        <div class="logo">
                            <img src="/Content/images/kyniem25/img/logo.png">
                        </div>
                        <div class="kyniem">
                            <img src="/Content/images/kyniem25/img/kyniem.png">
                        </div>
                        <div class="register">
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-6 col-md-6">
                                        @Html.TextBoxFor(m => m.txthoten, new { @class = "form-control pull-left inputtext", placeholder = "Họ và tên", type = "text" })


                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-3 col-md-3">
                                        @Html.TextBoxFor(m => m.noidung, new { @class = "form-control pull-left inputtext", placeholder = "Mã nhân viên", type = "number" })


                                    </div>
                                    <div class="col-3 col-md-3">
                                        @Html.DropDownListFor(m => m.Dichvu, this.ViewBag.GetDichvu as List<SelectListItem>, new { @class = "form-control inputtext" })



                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    @if (!string.IsNullOrEmpty(Model.txtError))
                                    {
                                        if (Model.txtError.Length > 0)
                                        {
                                            <div class="col-6 col-md-6" style="text-align:center;margin-bottom:8px;">
                                                <span style="margin-bottom:10px; color:white;text-align:center;font-size:28px; ">
                                                    @Model.txtError
                                                </span>
                                            </div>
                                        }
                                    }

                                    <div class="col-6 col-md-6">


                                        <button type="submit" name="submit" class="btn btn-submit--from" id="guiYeuCau" value="SaveAdd">CHECKIN</button>


                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="link_down">
                            <a href="/Content/images/kyniem25/file/filexem_nen.pdf" download="Sách kỷ yếu 25 năm" target="_blank " class="col-md-3 col-lg-3 ">
                                <img src="/Content/images/kyniem25/img/down.png">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    }

