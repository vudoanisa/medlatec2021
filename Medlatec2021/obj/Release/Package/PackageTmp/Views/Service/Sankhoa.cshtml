@using MEDLATEC.BusinessLayer;
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
    var TinNoiBat = (List<Cms_News>)ViewBag.TinNoiBat;
    var NewsCate = (List<Cms_News>)ViewBag.NewsCate;
}

@section Note {
    <title>Sản khoa | Bệnh viện đa khoa Medlatec</title>

    <meta name="description" content="Sản phụ khoa thuộc bệnh viện đa khoa medlatec, chuyên sản khoa và phụ khoa. Medlatec Quy tụ nhiều bác sĩ đầu ngành về lĩnh vực sản phụ khoa ở trong và ngoài nước" />
    <meta property="og:description" content="Sản phụ khoa thuộc bệnh viện đa khoa medlatec, chuyên sản khoa và phụ khoa. Medlatec Quy tụ nhiều bác sĩ đầu ngành về lĩnh vực sản phụ khoa ở trong và ngoài nước" />
    <meta name="title" content="Sản khoa | Bệnh viện đa khoa Medlatec" />
    <meta property="og:title" content="Sản khoa | Bệnh viện đa khoa Medlatec" />
    <meta name="keywords" content="sản khoa, khám sản, sản phụ khoa, phụ khoa, phụ nữ, sản phụ" />







    <meta property="og:image" content="https://medlatec.vn/Content/images/Chuyen-khoa/chuyen-khoa-san.jpg" />


    <meta property="og:type" content="article" />
    <meta property="fb:app_id" content="2092985977431269" />
    <meta property="og:url" content="https://medlatec.vn/chuyen-khoa/san-khoa" />



    <meta name="robots" content="index,follow,all">
    <link rel="canonical" href="https://medlatec.vn/chuyen-khoa/san-khoa" />
}

@using (Html.BeginForm("Sankhoa", "Service", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
{


  
    <div class="breadcrumb-container container">
        <ul class="clearfix">
            <li><a href="https://medlatec.vn">Trang chủ</a></li>
            <li><a href="https://medlatec.vn/chuyen-khoa">Chuyên khoa y tế</a></li>
            <li><a href="#">Chuyên khoa sản</a></li>

        </ul>
    </div>

    <div class="specialist specialist-child container">
        <div class="specialist-child--row row">
            <div class="specialist-child--col col-xl-9 col-lg-8">
                <h1>Sản khoa</h1>
                <div class="specialist-child--desc">Được xác định là một trong những chuyên khoa mũi nhọn, chuyên khoa Sản - Phụ khoa của Bệnh viện Đa khoa MEDLATEC đã không ngừng chú trọng hội tụ đội ngũ chuyên gia, bác sĩ đầu ngành, giàu kinh nghiệm, cùng đầy đủ hệ thống máy móc hiện đại để xứng đáng là địa chỉ tin cậy của chị em phụ nữ trong nhiều năm qua.</div>
                <div class="specialist-child--img"><img src="/Content/images/Chuyen-khoa/chuyen-khoa-san.jpg" alt="Chuyên khoa sản" /></div>
                <div class="specialist-service">
                    <h2 class="main-title">Dịch vụ chính</h2>
                    <div class="specialist-service--wrapper">
                        <div class="specialist-service--list row">
                            <div class="specialist-service--col col-lg-6">
                                <div class="specialist-service-item">
                                    <h3>Khám và điều trị các bệnh lý phụ khoa</h3>
                                    <p>Viêm sinh dục, chẩn đoán điều trị u buồng trứng, u xơ tử cung, lạc nội mạc tử cung, viêm phần phụ,...</p>
                                </div>
                            </div>
                            <div class="specialist-service--col col-lg-6">
                                <div class="specialist-service-item">
                                    <h3>Hỗ trợ sinh sản</h3>
                                    <p>Tư vấn, điều trị và bơm IUI.</p>
                                </div>
                            </div>
                            <div class="specialist-service--col col-lg-6">
                                <div class="specialist-service-item">
                                    <h3>Khám, quản lý và theo dõi thai</h3>
                                    <p>Điều trị sảy thai liên tiếp, sảy thai, thai lưu liên tiếp,… </p>
                                </div>
                            </div>
                            <div class="specialist-service--col col-lg-6">
                                <div class="specialist-service-item">
                                    <h3>Khám quản lý thai</h3>
                                    <p>Điều trị doạ sảy thai, sảy thai, thai lưu liên tiếp,...</p>
                                </div>
                            </div>
                            <div class="specialist-service--col col-lg-6">
                                <div class="specialist-service-item">
                                    <h3>Tầm soát các loại ung thư của nữ giới</h3>
                                    <p>Ung thư cổ tử cung, buồng trứng, vú,…</p>
                                </div>
                            </div>
                            <div class="specialist-service--col col-lg-6">
                                <div class="specialist-service-item">
                                    <h3>Kế hoạch hóa gia đình</h3>
                                    <p>Đặt vòng tránh thai, cấy – tháo que tránh thai</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>



                <div class="specialist-material" style="margin-top:15px;">
                    <h2 class="main-title">Cơ sở vật chất - Trang thiết bị</h2>
                    <div class="specialist-material-desc">Cùng với cơ sở hạ tầng khang trang, hiện đại, chuyên khoa Sản – Phụ khoa đã được trang bị đồng bộ hệ thống máy móc hiện đại và bảo đảm đáp ứng đầy đủ các điều kiện cần thiết theo yêu cầu của Bộ Y tế, cụ thể:</div>
                    <div class="specialist-material-wrapper">
                        <div class="specialist-material-item">
                            <div class="specialist-material--row row">
                                <div class="specialist-material--col col-xl-3 col-sm-4">
                                    <div class="specialist-material--img">
                                        <img src="/Content/images/Chuyen-khoa/may-noi-soi-co-tu-cung.png" alt="Máy nội soi cổ tử cung (CTC)" />
                                    </div>
                                </div>
                                <div class="specialist-material--col col-xl-9 col-sm-8">
                                    <h3>Máy nội soi cổ tử cung (CTC)</h3>
                                    <div class="specialist-material-intro d-none d-sm-block">
                                        <p>Khi thực hiện soi máy chiếu một luồng ánh sáng vào âm đạo và vào cổ tử cung. Qua đó, hình ảnh truyền về qua máy soi sẽ được phóng đại lên rất nhiều lần. Từ những hình ảnh soi trực tiếp rõ nét, bác sĩ dễ dàng quan sát các bất thường ở cổ tử cung của bệnh nhân mà mắt thường thậm khó có thể nhìn thấy được. </p>
                                        <p>Hiện nay, máy này là một kỹ thuật được dùng để sàng lọc, phát hiện sớm tiền ung thư, ung thư CTC.</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="specialist-material-item">
                            <div class="specialist-material--row row">
                                <div class="specialist-material--col col-xl-3 col-sm-4">
                                    <div class="specialist-material--img">
                                        <img src="/Content/images/Chuyen-khoa/may-cat-doi-leep-ctc.png" alt="Máy cắt đốt, LEEP CTC (xuất xứ tại Ý)" />
                                    </div>
                                </div>
                                <div class="specialist-material--col col-xl-9 col-sm-8">
                                    <h3>Máy cắt đốt, LEEP CTC (xuất xứ tại Ý)</h3>
                                    <div class="specialist-material-intro d-none d-sm-block">
                                        <p>Là máy cắt đốt tổn thương CTC hoặc điều trị tổn thương tiền ung thư CTC rất nhanh, hiệu quả.</p>
                                        <p>Máy chuyên dụng này có những ưu điểm như khi thực hiện thủ thuật, bệnh nhân được gây tê nên không đau, không chảy máu, không nhiễm trùng. Thời gian thực hiện nhanh chóng từ 10-20 phút, an toàn, không ảnh hưởng đến sức khỏe sinh sản, thực hiện xong bệnh nhân chỉ cần nghỉ ngơi và được ra viện ngay trong ngày.</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="specialist-material-item">
                            <div class="specialist-material--row row">
                                <div class="specialist-material--col col-xl-3 col-sm-4">
                                    <div class="specialist-material--img">
                                        <img src="/Content/images/Chuyen-khoa/may-logiq-f6.png" alt="Máy LOGIQ F6 (xuất xứ từ Mỹ)" />
                                    </div>
                                </div>
                                <div class="specialist-material--col col-xl-9 col-sm-8">
                                    <h3>Máy LOGIQ F6 (xuất xứ từ Mỹ)</h3>
                                    <div class="specialist-material-intro d-none d-sm-block">
                                        <p>Máy siêu âm 4D cho chất lượng hình ảnh vượt trội (rõ nét, độ phân giải cao) trong phát hiện bất thường.</p>
                                        <p>Ngoài ra, chuyên khoa còn sự hỗ trợ của các thiết bị chẩn đoán hình ảnh khác như: Siêu âm thai, siêu âm phụ khoa, siêu âm bơm nước buồng tử cung, chụp mammoghaphy (X-quang tuyến vú), chụp X-quang tử cung tử cung – vòi trứng,…</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="specialist-material-item">
                            <div class="specialist-material--row row">
                                <div class="specialist-material--col col-xl-3 col-sm-4">
                                    <div class="specialist-material--img">
                                        <img src="/Content/images/specialist-material.png" alt="" />
                                    </div>
                                </div>
                                <div class="specialist-material--col col-xl-9 col-sm-8">
                                    <h3>Máy nội soi cổ tử cung</h3>
                                    <div class="specialist-material-intro d-none d-sm-block">
                                        <p>Phát hiện tổn thương cổ tử cung giúp sàng lọc, phát hiện sớm tiền ung thư, ung thư CTC.</p>
                                        <p>Hình ảnh soi trực tiếp nét, bệnh nhân có thể trực tiếp quan sát cùng bác sỹ khi thực hiện nội soi và có ảnh in rất rõ nét.</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="single-news--col col-xl-3 col-lg-4 d-none d-lg-block">
                <div class="sidebar-container">
                    <div class="news-post">
                        <p style="font-size: 21px;line-height: 21px;color:#1576C4;font-weight:600;text-align:center;text-transform:uppercase;">Tin cùng chuyên mục</p>
                        <div class="news-post--list">
                            @if (NewsCate != null)
                            {
                                for (int i = 0; i < NewsCate.Count; i++)
                                {

                                    if (i < 5)
                                    {
                                        <div class="news-post--item">
                                            <div class="news-post--row row">
                                                <div class="news-post--col col-5 col-sm-4 col-md-5">
                                                    <div class="news-post--img">

                                                        @if (NewsCate[i].NewsImages != null)
                                                        {
                                                            <a href="/@CMS_Core.Common.Common.GetURLDetailByNews("tin-tuc",@NewsCate[i].NewsName.ToString().ToLower(),"s"+@NewsCate[i].CateId.ToString() ,"n"+@NewsCate[i].NewsId.ToString())" title="@NewsCate[i].NewsName.ToString().TrimStart().TrimEnd()"><img src="@NewsCate[i].NewsImages.ToString()" alt="@NewsCate[i].NewsName.ToString().TrimStart().TrimEnd()" /></a>
                                                        }


                                                    </div>
                                                </div>
                                                <div class="news-post--col col-7 col-sm-8 col-md-7">
                                                    <div class="news-post--content">

                                                        @if (@NewsCate[i].NewsName != null)
                                                        {
                                                            <h3><a href="/@CMS_Core.Common.Common.GetURLDetailByNews("tin-tuc",@NewsCate[i].NewsName.ToString().ToLower(),"s"+@NewsCate[i].CateId.ToString() ,"n"+@NewsCate[i].NewsId.ToString())" title="@NewsCate[i].NewsName.ToString().TrimStart().TrimEnd()">@NewsCate[i].NewsName.ToString().TrimStart().TrimEnd()</a></h3>
                                                        }

                                                        <div class="news-post--info clearfix">
                                                            @if (@NewsCate[i].DateCreate != null)
                                                            {
                                                                <span class="news-post--date">Ngày @NewsCate[i].DateCreate.ToString("dd/MM/yyyy")</span>
                                                            }


                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    }


                                }
                            }

                        </div>
                    </div>

                </div>

                <div class="sidebar-container">
                    <div class="news-post">
                        <p style="font-size: 21px;line-height: 21px;color:#1576C4;font-weight:600;text-align:center;text-transform:uppercase;">Tin nổi bật</p>
                        <div class="news-post--list">

                            @for (int i = 0; i < 5; i++)
                            {



                                <div class="news-post--item">
                                    <div class="news-post--row row">
                                        <div class="news-post--col col-5 col-sm-4 col-md-5">
                                            <div class="news-post--img">

                                                @if (@TinNoiBat[i].NewsImages != null)
                                                {
                                                    <a href="/@CMS_Core.Common.Common.GetURLDetailByNews("tin-tuc",@TinNoiBat[i].NewsName.ToString().ToLower(),"s"+@TinNoiBat[i].CateId.ToString() ,"n"+@TinNoiBat[i].NewsId.ToString())" title="@TinNoiBat[i].NewsName.ToString().TrimStart().TrimEnd()"><img src="@TinNoiBat[i].NewsImages.ToString()" alt="@TinNoiBat[i].NewsName.ToString().TrimStart().TrimEnd()" /></a>
                                                }


                                            </div>
                                        </div>
                                        <div class="news-post--col col-7 col-sm-8 col-md-7">
                                            <div class="news-post--content">

                                                @if (@TinNoiBat[i].NewsName != null)
                                                {
                                                    <h3><a href="/@CMS_Core.Common.Common.GetURLDetailByNews("tin-tuc",@TinNoiBat[i].NewsName.ToString().ToLower(),"s"+@TinNoiBat[i].CateId.ToString() ,"n"+@TinNoiBat[i].NewsId.ToString())" title="@TinNoiBat[i].NewsName.ToString().TrimStart().TrimEnd()">@TinNoiBat[i].NewsName.ToString().TrimStart().TrimEnd()</a></h3>
                                                }

                                                <div class="news-post--info clearfix">
                                                    @if (@TinNoiBat[i].DateCreate != null)
                                                    {
                                                        <span class="news-post--date">Ngày @TinNoiBat[i].DateCreate.ToString("dd/MM/yyyy")</span>
                                                    }


                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>

                </div>


            </div>

        </div>
    </div>

    @Html.AntiForgeryToken()
    <div class="register-main single-register-main container">
        <div class="register-wrapper">
            <div class="register-row row">
                <div class="register-col col-lg-6">
                    <div class="register-form">
                        <p style="font-size: 21px;line-height: 21px;color:white;text-align:center;text-transform:uppercase;">Đăng ký khám, tư vấn</p>
                        <div class="form">

                            <style type="text/css">
                                input[name='CaptchaInputText'] {
                                    width: 100% !important;
                                    line-height: 28px;
                                    background: rgba(255, 255, 255, .8) none;
                                    padding: 10px;
                                    height: 50px;
                                    display: block;
                                    width: 100%;
                                    font-size: 1rem;
                                    font-weight: 400;
                                    line-height: 1.5;
                                    color: #495057;
                                    border: 1px solid #ced4da;
                                    border-radius: .25rem;
                                    transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
                                }
                            </style>

                            <div class="form-group register-name">
                                <label>Họ và tên <i>(*)</i></label>
                                <input value="" name="txthoten" class="form-control" type="text" />
                            </div>
                            <div class="form-group register-phone">
                                <label>Số điện thoại <i>(*)</i></label>
                                <input value="" name="txtsdt" class="form-control" type="text" />
                            </div>
                            <div class="form-group register-email">
                                <label>Email</label>
                                <input value="" name="txtemail" class="form-control" type="email" />
                            </div>
                            <div class="form-group register-comment">
                                <label>Ghi chú khác</label>
                                <textarea class="form-control" name="txtGhichu" placeholder="Nhu cầu khám của bạn là gì? (không bắt buộc)"></textarea>
                            </div>


                            <div class="form-group form-group--btn">
                                <button type="submit" name="submit" value="SaveAdd">ĐĂNG KÝ</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="register-col col-lg-6 d-none d-lg-block">
                    <div class="choose-main">
                        <p class="line-title">Tại sao nên chọn bệnh viện đa khoa MEDLATEC</p>
                        <div class="choose-main--row row">
                            <div class="choose-main--col col-lg-6">
                                <div class="choose-main--item">
                                    <div class="choose-main--icon"><img src="/Content/Content/images/hospital.png" alt="" /></div>
                                    <div class="choose-main--desc">Bệnh viện đa khoa với nhiều năm kinh nghiệm trong lĩnh vực y tế.</div>
                                </div>
                            </div>
                            <div class="choose-main--col col-lg-6">
                                <div class="choose-main--item">
                                    <div class="choose-main--icon"><img src="/Content/Content/images/doctors.png" alt="" /></div>
                                    <div class="choose-main--desc">Thăm khám bởi đội ngũ giáo sư, bác sĩ đầu ngành</div>
                                </div>
                            </div>
                            <div class="choose-main--col col-lg-6">
                                <div class="choose-main--item">
                                    <div class="choose-main--icon"><img src="/Content/Content/images/infrastructure.png" alt="" /></div>
                                    <div class="choose-main--desc">Cơ sở vật chất, máy móc trang thiết bị hiện đại</div>
                                </div>
                            </div>
                            <div class="choose-main--col col-lg-6">
                                <div class="choose-main--item">
                                    <div class="choose-main--icon"><img src="/Content/Content/images/bill.png" alt="" /></div>
                                    <div class="choose-main--desc">Áp dụng thanh toán bảo hiểm y tế và các bảo hiểm khác lên tới 100%</div>
                                </div>
                            </div>
                            <div class="choose-main--col col-lg-6">
                                <div class="choose-main--item">
                                    <div class="choose-main--icon"><img src="/Content/Content/images/fast.png" alt="" /></div>
                                    <div class="choose-main--desc">Quy trình khám chữa bệnh nhanh chóng, thuận tiện</div>
                                </div>
                            </div>
                            <div class="choose-main--col col-lg-6">
                                <div class="choose-main--item">
                                    <div class="choose-main--icon"><img src="/Content/Content/images/dollar.png" alt="" /></div>
                                    <div class="choose-main--desc">Chi phí khám chữa bệnh hợp lý.</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

}