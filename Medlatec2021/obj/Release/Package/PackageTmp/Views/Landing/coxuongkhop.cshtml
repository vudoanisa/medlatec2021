                        </div>
                        <div class="form-group">
                            @Html.TextBoxFor(m => m.txtEmail, new { @class = "form-control", placeholder = "Email", type = "email" })

                        </div>
                        <div class="form-group">
                            @Html.TextAreaFor(m => m.txtnote, new { @class = "form-control", placeholder = "Ghi chú", type = "text" })

                        </div>


                        <div class="form-group form-group--btn" style="margin-bottom:20px;">
