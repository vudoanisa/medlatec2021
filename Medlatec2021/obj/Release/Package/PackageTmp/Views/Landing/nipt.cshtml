    @using (Html.BeginForm("nipt", "Landing", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal" }))
    {
        @Html.AntiForgeryToken()

        <footer>
            <div class="container fix-container">
                <div class="all-from-number">
                    <div class="fom-data-text">
                        <div class="title-dk">
                            <p class="text">Đăng ký ngay</p>
                        </div>
                        <div class="form" id="frmRegsiter">

                            <div class="input-name name-top">
                                <label style="margin-bottom: 5px" for="lbl-name">Họ và Tên(*)</label>
                                @Html.TextBoxFor(m => m.txthoten, new { @class = "form-control className", autocomplete = "off", placeholder = "Họ và Tên", type = "text" })



                            </div>
                            <div class="input-name">
                                <label style="margin-bottom: 5px" for="lbl-name">Số điện thoại(*)</label>
                                @Html.TextBoxFor(m => m.txtdt, new { @class = "form-control classSdt", placeholder = "Số điện thoại", autocomplete = "off", type = "text" })


                            </div>
                            <div class="input-name">
                                <label style="margin-bottom: 5px" for="lbl-name">Địa chỉ </label>
                                @Html.TextBoxFor(m => m.txtAddress, new { @class = "form-control classAdress", placeholder = "Địa chỉ", autocomplete = "off", type = "text" })
                            </div>
                            <div class="input-name">
                                <label style="margin-bottom: 5px" for="lbl-name">Gói dịch vụ</label>
                                @Html.DropDownListFor(m => m.Dichvu, this.ViewBag.GetDichvu as List<SelectListItem>, new { @class = "form-control tab-customer--location", style = "text-align-last:center;" })



                            </div>
                            <div class="input-name">
                                <label style="margin-bottom: 5px" for="lbl-name">Ghi chú</label>
                                @Html.TextBoxFor(m => m.txtnote, new { @class = "form-control classnote", placeholder = "Ghi chú", autocomplete = "off", type = "text" })


                            </div>
                            <div class="btn-from">

                                <button type="submit" name="submit" class="btn btn-default btn-submit-from" id="IDfromsubmit" value="SaveAdd">GỬI YÊU CẦU</button>


                                <!--<p class="btn btn-default btn-submit-from" id="IDfromsubmit">Gửi yêu cầu</p>-->
                            </div>


                        </div>
                    </div>
                    <div class="MEDLATEC-bg-phone btn btn-default btn-phone-bottom">
