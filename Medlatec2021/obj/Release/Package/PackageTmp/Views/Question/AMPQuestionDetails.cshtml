                                            noidung = noidung.Replace("<br />", "");
                                            noidung = noidung.Replace("<br/>", "");
                                            noidung = noidung.Replace("<br type=\"_moz\" />", "");
                                            noidung = noidung.Replace("<br type=\"_moz\"/>", "");
                                            <div class="faq-reply-content">
                                                @Html.Raw(noidung)
                                            </div>
                                        }
                                    }
                                }



                            </div>
                        </div>

